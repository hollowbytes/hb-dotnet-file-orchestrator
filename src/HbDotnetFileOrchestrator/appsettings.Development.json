{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
              }
            }
          ]
        }
      }
    ]
  },
  "Authentication": {
    "BlobStorage": {
      "DefaultAzurite": {
        "ConnectionString": "foo"
      }
    }
  },
  "Storage": {
    "BlobStorage": [],
    "Sftp": [],
    "FileSystem": [
      {
        "Id": "foo",
        "Rule": "metadata.Query.Any(x => x.Key == \"foo\")",
        "Destination": "./foo/{{ metadata.Query.Key[0] }}"
      }
    ]
  }
}
